<Page x:Class="Board.pageClipboard"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Board"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="frameClipboard">

    <Grid>
        <Rectangle RadiusX="20" RadiusY="20" Fill="White" Stroke="DodgerBlue">

        </Rectangle>
        <ListView x:Name="lvClipboard" Margin="20" BorderThickness="0" MouseDoubleClick="lvClipboard_MouseDoubleClick" KeyDown="lvClipboard_KeyDown"/>

    </Grid>
</Page>
